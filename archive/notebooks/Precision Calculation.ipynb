    "import pandas as pd\n",
    "\n",
    "data = pd.read_csv('confusion_matrix_CARSHZD.csv', header=None)\n",
    "\n",
    "total_sum = data.values.sum()\n",
    "\n",
    "diagonal_sum = data.values.trace()\n",
    "\n",
    "precision = diagonal_sum / total_sum\n",
    "\n",
    "print(\"Precision:\", precision)\n"
   ]
